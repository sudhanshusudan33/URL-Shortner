{
    "name": "URL Shortener",
    "version": "1.0.0",
    "description": "Create a tiny URL",
    "author": "Sudhanshu",
    "manifest_version": 3,
  
    "icons": {
      "16": "logo16.png",
      "48": "logo48.png"
    },
  
    "action": {
      "default_popup": "index.html"
    },
  
    "options_ui": {
      "page": "options.html",
      "open_in_tab": false
    },
  
    "permissions": [
      "storage"
    ]
  }
  